<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/verde4"
    android:backgroundTint="@color/verde4"
    android:foregroundTint="@color/verde4"
    tools:context=".MenuApp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:elevation="30dp"
        android:layout_width="match_parent"
        android:layout_height="300sp"
        android:background="@drawable/text_bord_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="157dp"
            android:layout_height="133dp"
            android:layout_marginStart="127dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="127dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/se_survey_removebg_preview" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="115dp"
            android:layout_marginEnd="115dp"
            android:gravity="center"
            android:text="SE Survey"
            android:textColor="@color/colorverde"
            android:textSize="40sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="98dp"
            android:layout_marginEnd="98dp"
            android:text="Datos para tu seguridad"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageButton
        android:id="@+id/btnUsuario"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="65dp"
        android:background="@drawable/button_border"
        android:backgroundTint="@color/colorverde"
        android:elevation="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnEncuestar"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        app:srcCompat="@drawable/img_btn_perfil" />

    <ImageButton
        android:id="@+id/btnEncuestar"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/button_border"
        android:backgroundTint="@color/colorverde"
        android:elevation="15dp"
        app:layout_constraintEnd_toStartOf="@+id/btnUsuario"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        app:srcCompat="@drawable/img_btn_list" />

    <ImageButton
        android:id="@+id/btnCerrarSesion"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="65dp"
        android:elevation="15dp"
        android:background="@drawable/button_border"
        android:backgroundTint="@color/colorverde"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnInformacion"
        app:layout_constraintTop_toBottomOf="@+id/textView48"
        app:srcCompat="@drawable/img_btn_salir" />

    <ImageButton
        android:id="@+id/btnInformacion"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/button_border"
        android:backgroundTint="@color/colorverde"
        android:elevation="15dp"
        app:layout_constraintEnd_toStartOf="@+id/btnCerrarSesion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:srcCompat="@drawable/img_btn_informacion" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:elevation="15dp"
        android:text="Encuesta"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnEncuestar"
        app:layout_constraintStart_toStartOf="@+id/btnEncuestar"
        app:layout_constraintTop_toBottomOf="@+id/btnEncuestar" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:elevation="15dp"
        android:text="Cerrar Sesión"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnCerrarSesion"
        app:layout_constraintStart_toStartOf="@+id/btnCerrarSesion"
        app:layout_constraintTop_toBottomOf="@+id/btnCerrarSesion" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:elevation="15dp"
        android:text="Información"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnInformacion"
        app:layout_constraintStart_toStartOf="@+id/btnInformacion"
        app:layout_constraintTop_toBottomOf="@+id/btnInformacion" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:elevation="15dp"
        android:text="Perfil"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnUsuario"
        app:layout_constraintStart_toStartOf="@+id/btnUsuario"
        app:layout_constraintTop_toBottomOf="@+id/btnUsuario" />

</androidx.constraintlayout.widget.ConstraintLayout>